{
    "name": "gelectron",
    "version": "1.0.0",
    "main": "./dist/main/main.js",
    "license": "MIT",
    "scripts": {
        "build:renderer": "webpack --config webpack.config.renderer.js",
        "build:main": "tsc",
        "build": "npm run build:main && npm run build:renderer",
        "start": "electron . --enable-logging",
        "compile:electron": "npm link ../electron-overlay && electron-rebuild -v 16.0.10 && npm run electron-overlay",
        "copy_binary": "cd .. && copy_binary.bat"
    },
    "devDependencies": {
        "node-addon-api": "^4.1.0",
        "awesome-typescript-loader": "^5.2.0",
        "electron-rebuild": "^3.2.7",
        "typescript": "^3.1.0",
        "webpack": "^4.16.5",
        "webpack-cli": "^3.1.0"
    },
    "dependencies": {
        "electron": "16.2.6",
        "electron-overlay": "file:../electron-overlay",
        "node-ovhook": "file:../node-ovhook"
    }
}
